{
  "name": "my-truck-api",
  "version": "0.0.1",
  "description": "Backend API pour My Truck Transport",
  "author": "My Truck / Digiltiz√®me",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "npx prisma db push && node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:api": "node scripts/test-api.js",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e:reset": "ts-node ./test/reset-test-db.ts",
    "test:e2e": "npm run test:e2e:reset && jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "jest --config ./jest-e2e.json --watch",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "extract": "ts-node scripts/extract-airtable.ts extract",
    "migrate:data": "ts-node scripts/extract-airtable.ts migrate",
    "migrate:data:new": "ts-node scripts/migrate-from-airtable.ts",
    "migrate:extensible": "ts-node scripts/production/extensible-migration-system.ts",
    "setup:dev": "npm run db:reset && npm run db:seed",
    "debug:airtable": "node scripts/debug-airtable.js",
    "debug:airtable:extended": "node scripts/debug-airtable-extended.js",
    "list:airtable:tables": "node scripts/archive/debug/list-airtable-tables.js",
    "extract:robust": "ts-node scripts/production/extract-airtable-robust.ts extract",
    "extract:single": "ts-node scripts/extract-airtable-robust.ts single",
    "system-health:check": "ts-node scripts/maintenance/system-health-check.ts check",
    "dev:backend": "npm run start:dev",
    "dev:frontend": "cd ../frontend && npm run dev",
    "dev:tunnel": "ngrok http 3000",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"sleep 5 && npm run dev:tunnel\""
  },
  "dependencies": {
    "@nestjs/common": "^10.4.19",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.19",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.19",
    "@nestjs/platform-socket.io": "^10.4.19",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^8.0.7",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.4.19",
    "@types/geojson": "^7946.0.14",
    "airtable": "^0.12.2",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.7.0",
    "date-fns": "^4.1.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfjs-dist": "^5.1.91",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1",
    "@prisma/client": "^6.15.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/date-fns": "^2.5.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jspdf": "^1.3.3",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.17.6",
    "@types/nodemailer": "^6.4.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pdfjs-dist": "^2.10.377",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "concurrently": "^8.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^6.14.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
